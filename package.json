{
  "name": "@polybase/changelog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@choc-ui/chakra-autocomplete": "^5.1.4",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@polybase/auth": "^0.3.39",
    "@polybase/client": "^0.3.39",
    "@polybase/eth": "^0.3.39",
    "@polybase/react": "^0.3.39",
    "@polybase/util": "^0.3.39",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "framer-motion": "^10.12.2",
    "nanoid": "^4.0.2",
    "posthog-js": "^1.53.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.8.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.0.4",
    "web-vitals": "^3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "load": "ts-node ./scripts/create-schema.ts",
    "env": "env-cmd --no-override --file ${ENV_PATH:-.env.development}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": [
        2,
        {
          "skipUndeclared": true
        }
      ],
      "react/jsx-handler-names": "off",
      "quotes": "off",
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "react/display-name": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "space-before-function-paren": 0,
      "arrow-body-style": 0,
      "no-use-before-define": 0,
      "arrow-parens": 0,
      "no-trailing-spaces": "error",
      "@typescript-eslint/quotes": [
        "error",
        "single"
      ],
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "keyword-spacing": "error",
      "key-spacing": [
        "error",
        {
          "mode": "strict"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "ts-node": "^10.9.1"
  }
}